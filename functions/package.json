{
  "name": "ios-ad-hoc-apps",
  "version": "1.0.0",
  "description": "Google Cloud Function для получения списка iOS приложений",
  "main": "index.js",
  "scripts": {
    "start": "functions-framework --target=getApps",
    "deploy": "gcloud functions deploy getApps --runtime nodejs18 --trigger-http --allow-unauthenticated"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.0.0"
  },
  "engines": {
    "node": "18"
  }
}
